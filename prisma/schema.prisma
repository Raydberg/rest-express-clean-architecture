generator client {
  provider     = "prisma-client"
  output       = "../src/config/lib/generated"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model todo {
  id          Int       @id @default(autoincrement())
  text        String    @db.VarChar()
  completedAt DateTime? @db.Timestamp()
}
